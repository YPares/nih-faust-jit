    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'nih_faust_jit'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=nih_faust_jit"
                ],
                "filter": {
                    "name": "nih_faust_jit",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'nih_faust_jit_standalone'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=nih_faust_jit_standalone",
                    "--package=nih_faust_jit"
                ],
                "filter": {
                    "name": "nih_faust_jit_standalone",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach to running nih_faust_jit",
            "pid": "${command:pickMyProcess}"
        }
    ]
}